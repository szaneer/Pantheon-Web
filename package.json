{
  "name": "@pantheon/web-app",
  "version": "1.0.0",
  "description": "Web version of Pantheon - access remote models from any browser",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3003",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3001",
    "serve": "vite build && vite preview --host 0.0.0.0 --port 3001",
    "serve-http": "npm run build && node scripts/serve-http.js",
    "dev-http": "vite --host 0.0.0.0 --port 3001 --open"
  },
  "keywords": [
    "llm",
    "web",
    "remote",
    "ai",
    "chat"
  ],
  "author": "Pantheon Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.2.2",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "lucide-react": "^0.294.0",
    "postcss": "^8.4.32",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.8.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.3.6",
    "util": "^0.12.5",
    "uuid": "^9.0.1"
  }
}
